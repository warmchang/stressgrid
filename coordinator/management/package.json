{
  "name": "stressgrid-management",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.3.1",
    "filesize": "^3.6.1",
    "lodash": "^4.17.11",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.10.2",
    "react-bootstrap": "^0.32.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "3.1.0",
    "react-sparklines": "^1.7.0",
    "reconnecting-websocket": "^4.1.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "postbuild": "rm -rf ../priv/management && mkdir -p ../priv/management && cp -r build/* ../priv/management/",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/filesize": "^3.6.0",
    "@types/jest": "^23.3.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/react-sparklines": "^1.7.0",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "typescript": "^3.1.1"
  }
}
